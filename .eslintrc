{
  "extends": "react-app",
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-console": 1,
    "no-debugger": 1,
    "no-duplicate-case": 1,
    "no-empty": [1, {
      "allowEmptyCatch": true
    }],
    "no-ex-assign": 1,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": 1,
    "no-extra-semi": 1,
    "no-inner-declarations": 1,
    "no-irregular-whitespace": 1,
    "no-prototype-builtins": 1,
    "no-unsafe-negation": 1,
    "accessor-pairs": 1,
    "block-scoped-var": 1,
    "consistent-return": 1,
    "dot-notation": 1,
    "no-alert": 2,
    "no-else-return": 1,
    "no-empty-function": 1,
    "no-global-assign": 2,
    "no-magic-numbers": [1, {
      "ignore": [0, 1]
    }],
    "no-multi-spaces": [1, {
      "exceptions": {
        "Property": false
      }
    }],
    "no-proto": 1,
    "no-param-reassign": 1,
    "no-return-assign": 1,
    "no-void": 1,
    "wrap-iife": 1,
    "yoda": 1,
    "block-spacing": 1,
    "array-bracket-spacing": 1,
    "brace-style": [1, "stroustrup"],
    "comma-dangle": [1, "always-multiline"],
    "comma-spacing": [1, {
      "before": false,
      "after": true
    }],
    "comma-style": [1, "last"],
    "eol-last": [1, "always"],
    "func-call-spacing": 1,
    "func-style": [1, "expression", {
      "allowArrowFunctions": true
    }],
    "jsx-quotes": 1,
    "key-spacing": 1,
    "indent": [1, 2],
    "max-depth": [1, 3],
    "max-statements-per-line": [1, {
      "max": 1
    }],
    "no-negated-condition": 1,
    "no-multiple-empty-lines": [1, {
      "max": 1
    }],
    "no-mixed-spaces-and-tabs": 1,
    "no-nested-ternary": 1,
    "no-whitespace-before-property": 1,
    "object-property-newline": 1,
    "one-var-declaration-per-line": 1,
    "padded-blocks": [1, "never"],
    "quotes": 1,
    "semi-spacing": 1,
    "semi": 1,
    "sort-keys": [1, "asc", {
      "natural": true
    }],
    "sort-vars": 1,
    "space-before-blocks": 1,
    "space-before-function-paren": [1, {
      "named": "ignore"
    }],
    "space-infix-ops": 1,
    "space-unary-ops": 1,
    "wrap-regex": 1,
    "arrow-parens": 1,
    "arrow-spacing": 1,
    "generator-star-spacing": 1,
    "no-duplicate-imports": 1,
    "no-useless-computed-key": 1,
    "object-shorthand": 1,
    "prefer-const": 1,
    "prefer-rest-params": 1,
    "prefer-spread": 1,
    "prefer-template": 1,
    "require-yield": 1,
    "rest-spread-spacing": 1,
    "sort-imports": [1, {
      "memberSyntaxSortOrder": ["all", "multiple", "single", "none"]
    }]
  }
}
